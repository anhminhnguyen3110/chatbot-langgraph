# Test Environment Configuration
# This file is used for running tests with noop authentication

# Database
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/agent

# Authentication - NOOP for testing
AUTH_TYPE=noop

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Logging
LOG_LEVEL=INFO
ENV_MODE=LOCAL
LOG_VERBOSITY=standard

# LLM Providers (not needed for most tests)
OPENAI_API_KEY=sk-test-key
GEMINI_API_KEY=test-key
PRIMARY_MODEL_ID=google_genai/gemini-2.5-flash
FALLBACK_MODEL_ID=google_genai/gemini-2.0-flash

# Langfuse (optional for testing)
LANGFUSE_LOGGING=false
LANGFUSE_SECRET_KEY=sk-lf-test
LANGFUSE_PUBLIC_KEY=pk-lf-test
LANGFUSE_HOST=https://cloud.langfuse.com
